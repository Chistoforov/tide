# Stormglass API
STORMGLASS_API_KEY=your_api_key_here

# Beach coordinates (optional, defaults to Cascais)
BEACH_LATITUDE=38.6978
BEACH_LONGITUDE=-9.4203

# Cron secret for update endpoint (optional)
CRON_SECRET=your_secret_here

# Supabase Configuration
# Получите эти значения в настройках проекта на https://supabase.com/dashboard
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
